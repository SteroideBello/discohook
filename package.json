{
  "name": "discohook",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/main.js",
    "build": "pnpm run build-client && pnpm run build-server",
    "build-client": "cross-env NODE_ENV=production webpack --config webpack.client.js",
    "build-server": "cross-env NODE_ENV=production webpack --config webpack.server.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.client.js",
    "test": "cross-env TZ=UTC jest --config .jestrc.json",
    "test:watch": "cross-env TZ=UTC jest --config .jestrc.json --watchAll",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src/**/*",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch",
    "format": "prettier --write ./src/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaylineko/discohook.git"
  },
  "author": "Jay <jaylineko@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaylineko/discohook/issues"
  },
  "homepage": "https://github.com/jaylineko/discohook#readme",
  "dependencies": {
    "@emotion/core": "^10.0.20",
    "@emotion/styled": "^10.0.17",
    "@emotion/styled-base": "^10.0.19",
    "core-js": "^3.2.1",
    "emotion-theming": "^10.0.19",
    "highlight.js": "^9.15.10",
    "koa": "^2.8.2",
    "koa-conditional-get": "^2.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "regenerator-runtime": "^0.13.3",
    "simple-markdown": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/highlight.js": "^9.12.3",
    "@types/jest": "^24.0.18",
    "@types/koa": "^2.0.50",
    "@types/koa-conditional-get": "^2.0.0",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/node": "^12.7.11",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.20",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "jest": "^24.9.0",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prettier": "^1.18.2",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  }
}
