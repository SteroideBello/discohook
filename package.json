{
  "name": "discohook",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node --max-http-header-size=81920 ./dist/main.js",
    "build": "pnpm run build:client && pnpm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.client.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.server.js",
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS='--max-http-header-size=81920' webpack-dev-server --config webpack.client.js",
    "test": "cross-env TZ=UTC NODE_ENV=test jest --config .jestrc.json",
    "test:watch": "cross-env TZ=UTC NODE_ENV=test jest --config .jestrc.json --watch",
    "test:cov": "cross-env TZ=UTC NODE_ENV=test jest --config .jestrc.json --coverage --collectCoverageFrom=\"src/**/*.{ts,tsx}\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src/**/*",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch",
    "format": "prettier --write ./src/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaylineko/discohook.git"
  },
  "author": "Jay <jaylineko@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaylineko/discohook/issues"
  },
  "homepage": "https://github.com/jaylineko/discohook#readme",
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@emotion/styled-base": "^10.0.24",
    "@koa/router": "^8.0.2",
    "core-js": "^3.4.1",
    "emotion-theming": "^10.0.19",
    "highlight.js": "^9.16.2",
    "isbot": "^2.5.2",
    "koa": "^2.11.0",
    "koa-conditional-get": "^2.0.0",
    "koa-static": "^5.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "regenerator-runtime": "^0.13.3",
    "simple-markdown": "^0.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@testing-library/dom": "^6.10.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/highlight.js": "^9.12.3",
    "@types/jest": "^24.0.23",
    "@types/koa": "^2.0.52",
    "@types/koa-conditional-get": "^2.0.0",
    "@types/koa-static": "^4.0.1",
    "@types/koa__router": "^8.0.2",
    "@types/node": "^12.12.8",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/testing-library__react": "^9.1.2",
    "@types/testing-library__user-event": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.23",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-unicorn": "^13.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prettier": "^1.19.1",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
